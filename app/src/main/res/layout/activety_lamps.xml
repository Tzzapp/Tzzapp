<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Formulas">

        <EditText
            android:id="@+id/editTextWallX"
            android:layout_width="90dp"
            android:layout_height="40dp"
            android:layout_marginTop="8dp"
            android:ems="10"
            android:enabled="false"
            android:inputType="number"
            app:layout_constraintEnd_toEndOf="@+id/spaceToWall"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/spaceToWall" />

        <EditText
            android:id="@+id/editTextWallY"
            android:layout_width="90dp"
            android:layout_height="40dp"
            android:layout_marginTop="8dp"
            android:ems="10"
            android:enabled="false"
            android:inputType="number"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/spaceToWall"
            app:layout_constraintTop_toBottomOf="@+id/spaceToWall" />

        <EditText
            android:id="@+id/editTextLampX"
            android:layout_width="90dp"
            android:layout_height="40dp"
            android:layout_marginTop="16dp"
            android:ems="10"
            android:enabled="false"
            android:inputType="number"
            app:layout_constraintEnd_toEndOf="@+id/spaceBetweenLamps"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/spaceBetweenLamps" />

        <TextView
            android:id="@+id/roomTittle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/room"
            android:textSize="25sp"
            android:textAlignment="center"
            android:layout_margin="50dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/lampTittle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="32dp"
            android:text="@string/lamp_description"
            android:textAlignment="center"
            android:textSize="25sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textfieldRoomXK" />

        <Button
            android:id="@+id/buttonCalculate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/calculate_button_text"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />


        <EditText
            android:id="@+id/editTextLampY"
            android:layout_width="90dp"
            android:layout_height="40dp"
            android:layout_marginTop="16dp"
            android:ems="10"
            android:enabled="false"
            android:inputType="number"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/spaceBetweenLamps"
            app:layout_constraintTop_toBottomOf="@+id/spaceBetweenLamps" />

        <TextView
            android:id="@+id/spaceBetweenLamps"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="24dp"
            android:text="@string/spaceBetweenLamps"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textfieldLampCountXK" />

        <TextView
            android:id="@+id/spaceToWall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="Plass mellom lampe og vegg"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextLampX" />

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/textfieldRoomXK"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="@string/roomLength"
            app:layout_constraintEnd_toEndOf="@+id/roomTittle"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/roomTittle">


            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/editTextRoomXK"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:ems="7"
                android:inputType="number"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="199dp" />
        </com.google.android.material.textfield.TextInputLayout>



        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/textfieldLampXK"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="@string/roomLength"
            app:layout_constraintEnd_toEndOf="@+id/lampTittle"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/lampTittle">


            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/editTextLampXK"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:ems="7"
                android:inputType="number"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="199dp" />
        </com.google.android.material.textfield.TextInputLayout>


        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/textfieldLampCountXK"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:hint="@string/count"
            app:layout_constraintEnd_toEndOf="@+id/lampTittle"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textfieldLampXK">


            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/editTextLampCountXK"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:ems="7"
                android:inputType="number"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="199dp" />
        </com.google.android.material.textfield.TextInputLayout>


        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/textfieldRoomYK"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="@string/roomWidth"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/roomTittle"
            app:layout_constraintTop_toBottomOf="@+id/roomTittle">


            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/editTextRoomYK"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:ems="7"
                android:inputType="number"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="199dp" />
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/textfieldLampYK"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:hint="@string/roomWidth"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/lampTittle"
            app:layout_constraintTop_toBottomOf="@+id/lampTittle">


            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/editTextLampYK"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:ems="7"
                android:inputType="number"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="199dp" />
        </com.google.android.material.textfield.TextInputLayout>


        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/textfieldLampCountYK"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:hint="@string/count"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/lampTittle"
            app:layout_constraintTop_toBottomOf="@+id/textfieldLampYK">


            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/editTextLampCountYK"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:ems="7"
                android:inputType="number"
                tools:layout_editor_absoluteX="16dp"
                tools:layout_editor_absoluteY="199dp" />
        </com.google.android.material.textfield.TextInputLayout>
        

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>